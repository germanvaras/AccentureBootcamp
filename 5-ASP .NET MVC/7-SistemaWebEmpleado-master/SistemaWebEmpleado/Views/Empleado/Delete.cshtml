

@model SistemaWebEmpleado.Models.Empleado

@{
    ViewBag.Title = "Delete";
}

<h2>Desea eliminar al Empleado @Model.Nombre @Model.Apellido ?</h2>


@using (Html.BeginForm("Delete", "Empleado", FormMethod.Post))
{
    <div>
        <spam>@Html.DisplayNameFor(model => model.Nombre)</spam>
        <spam>@Html.DisplayFor(model => model.Nombre)</spam>
        <spam>@Html.DisplayNameFor(model => model.Apellido)</spam>
        <spam>@Html.DisplayFor(model => model.Apellido)</spam>
        <spam>@Html.DisplayNameFor(model => model.Legajo)</spam>
        <spam>@Html.DisplayFor(model => model.Legajo)</spam>
        <spam>@Html.DisplayNameFor(model => model.DNI)</spam>
        <spam>@Html.DisplayFor(model => model.DNI)</spam>
        <spam>@Html.DisplayNameFor(model => model.Titulo)</spam>
        <spam>@Html.DisplayFor(model => model.Titulo)</spam>
    </div>
    <input type="submit" value="Eliminar" />
}

<p>
    @Html.ActionLink("Volver a la lista de Empleados", "Index", "Empleado")
</p>

